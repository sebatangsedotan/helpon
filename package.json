    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.2",
    "@types/validator": "^13.15.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
