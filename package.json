  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bun run build && bun run prod",
    "build": "bun run tsc && cp .env ./build/.env",
    "prod": "bun run node ./build/index.js",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed": "bun run tsx prisma/seed.ts",
    "prepare": "husky"
  },
  "keywords": [
    "zeinirfansyah",
    "helpon",
    "money management",
    "personal finance"
  ],
  "author": "zeinirfansyah",
  "license": "ISC",
  "description": "",
